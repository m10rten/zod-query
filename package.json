{
  "name": "zod-query",
  "version": "0.0.0",
  "license": "MIT",
  "description": "A query language for Zod schemas and your own database",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md,css,scss,mdx,json,yaml}\"",
    "packages:test": "turbo run test --filter=./packages/*",
    "packages:build": "turbo run build --filter=./packages/*",
    "packages:lint": "turbo run lint --filter=./packages/*",
    "packages:release": "changeset publish",
    "release:build": "changeset version",
    "release:add": "changeset",
    "release:pre": "changeset pre",
    "typecheck": "turbo run typecheck",
    "generate": "turbo run generate",
    "cli": "turbo run cli"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@changesets/cli": "2.27.1",
    "@types/jest": "29.5.11",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "0.5.9",
    "ts-jest": "29.1.1",
    "tsconfig": "workspace:*",
    "turbo": "1.11.2",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.16.0",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "nodemon": "3.0.2",
    "ts-node": "10.9.2",
    "zod": "3.22.4"
  },
  "packageManager": "pnpm@8.6.10"
}
