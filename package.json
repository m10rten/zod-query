{
  "name": "zod-query",
  "version": "0.0.0",
  "license": "MIT",
  "description": "A query language for Zod schemas and your own database",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,md,css,scss,mdx,json,yaml}\"",
    "packages:test": "turbo run test --filter=./packages/*",
    "packages:build": "turbo run build --filter=./packages/*",
    "packages:lint": "turbo run lint --filter=./packages/*",
    "packages:release": "changesets publish",
    "release:create": "changesets version",
    "release": "pnpm run packages:build && pmpm run packages:release",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "eslint": "8.50.0",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.5",
    "tsconfig": "workspace:*",
    "turbo": "1.10.15"
  },
  "packageManager": "pnpm@8.6.10"
}
